{
    "rules": {
        "indent": [
            0
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "comma-spacing": [
          2,
          {
            "before": false,
            "after": true
          }
        ],
        "no-console": [0]
    },
    "env": {
        "node": true,
        "jasmine": true
    },
    "extends": "eslint:recommended"
}
